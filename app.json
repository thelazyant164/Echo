{
  "expo": {
    "name": "Echo",
    "slug": "Echo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/Logov2.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/Logov2.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFFFFF"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*" 
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.Echo",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
